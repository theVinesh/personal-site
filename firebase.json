{
  "hosting": {
    "public": "app/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/pages/home.html"
      },
      {
        "source": "**",
        "destination": "pages/404.html"
      }
    ]
  }
}
