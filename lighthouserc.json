{
  "ci": {
    "collect": {
      "staticDistDir": "./dist",
      "numberOfRuns": 1
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "offscreen-images": "off",
        "uses-webp-images": "on",
        "works-offline": "off",
        "offline-start-url": "off",
        "uses-rel-preconnect": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}