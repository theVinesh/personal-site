{
  "ci": {
    "collect": {
      "staticDistDir": "./build/web",
      "numberOfRuns": 2
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "uses-webp-images": "on",
        "meta-viewport": "off",
        "redirects-http": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
